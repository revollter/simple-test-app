<table class="table table-hover">
    <thead>
    <tr>
        <th>Id</th>
        <th>User name</th>
        <th>Date</th>
        <th>Product</th>
        <th>Color</th>
        <th>Amount</th>
    </tr>
    </thead>
    <tbody>
    {% for item in data %}
        <tr>
            <td>{{ item.id }}</td>
            <td>{{ item.account.fullname }}</td>
            <td>{{ item.date | date("d/m/Y") }}</td>
            <td>{{ item.product.value }}</td>
            <td>{{ item.color ? item.color.value : "-" }}</td>
            <td>{{ item.amount }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>